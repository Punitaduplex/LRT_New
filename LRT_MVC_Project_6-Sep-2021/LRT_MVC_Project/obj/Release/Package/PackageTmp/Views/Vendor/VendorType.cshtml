            var oTable = $('#EmpInfovendortype').dataTable();
            var rowcollection = oTable.$("tbody>tr", { "page": "all" });
           // alert(rowcollection.length);
          //  return;
            rowcollection.each(function (index, elem) {
                
                var row = $(elem).closest("tr");
                //alert(row.find('td:eq(1)').text())
                if (objVendorType.Vendor_Type == "") {
                    objVendorType.Vendor_Type = row.find('td:eq(1)').text().charAt(0).toUpperCase() + row.find('td:eq(1)').text().slice(1);;
                }

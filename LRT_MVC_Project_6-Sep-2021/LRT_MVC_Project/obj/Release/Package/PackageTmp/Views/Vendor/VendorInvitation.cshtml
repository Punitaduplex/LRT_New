        var $row = $(this).closest("tr");    // Find the row
       
        var $id = $row.find('td:eq(5)').text();

        var objVendor = {};

        objVendor.Vendor_Invitation_Id = $id;

        $.ajax({
            type: "POST",
            url: "/Vendor/VendorInvitationForEdit",
            data: '{objVendor: ' + JSON.stringify(objVendor) + '}',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {
                $("#txtupcompanyname").val(response.Vendor_Company_Name);
                $("#txtupcontactperson").val(response.Vendor_Contact_Person);
                $("#txtupemailid").val(response.Vendor_Email_ID);

                $("#hdvendorregistration").val(response.Vendor_Invitation_Id);
               
//-----------------------------------------------------------------------------------
                //var objVendor = {};

                //objVendor.Vendor_Type_Id = $id;

                $.ajax({
                    type: "POST",
                    url: "/Vendor/VendorTypeAccInvitationId",
                    data: '{objVendor: ' + JSON.stringify(objVendor) + '}',
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (response) {
                       // alert();
                        $("input:checkbox[name=form-field-checkbox1]").each(function (index, value) {
                            var id=($(this).val());
                            for (var i = 0; i < response.length; i++) {
                                var checkedid = response[i].Vendor_Type_Id;
                                if (id == checkedid) {
                                    $("input[name=form-field-checkbox1][value='" + id + "']").prop('checked', true);
                                }
                            }
                        });
  
                    },
                    failure: function (response) {
                        alert(response.responseText);
                    },
                    error: function (response) {

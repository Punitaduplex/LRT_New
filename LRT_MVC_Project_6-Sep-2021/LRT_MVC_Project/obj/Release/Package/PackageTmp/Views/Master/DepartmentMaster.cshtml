<script type="text/javascript">
    $(function () {
        $("#btnsaveuploaddb").click(function () {
            $('#ExcelDataLoader').show();
            var objDepartment = {};
            objDepartment.Department_Name = "";
            objDepartment.Department_Description = "";
            // alert();
            objDepartment.Department_Id = 0;
            var oTable = $('#EmpInfodepartment').dataTable();
            var rowcollection = oTable.$("tr", { "page": "all" });

            rowcollection.each(function (index, elem) {
                var row = $(elem).closest("tr");
                if (objDepartment.Department_Name == "") {
                    objDepartment.Department_Name = row.find('td:eq(1)').text().toUpperCase().charAt(0).toUpperCase() + row.find('td:eq(1)').text().slice(1);
                    objDepartment.Department_Description = row.find('td:eq(2)').text();

                }
                else {
                    objDepartment.Department_Name = objDepartment.Department_Name + "," + row.find('td:eq(1)').text().charAt(0).toUpperCase() + row.find('td:eq(1)').text().slice(1);
                    objDepartment.Department_Description = objDepartment.Department_Description + "," + row.find('td:eq(2)').text();

                }
            });
            var rowCount = $('#EmpInfodepartment tr').length;
            if (rowCount <= 1) {
                swal("please upload excel file for data upload.");
                return;
            }
            if (rowCount > 1 && objDepartment.Department_Name == "") {
                swal("no data in table.");
                return;
            }

            $.ajax({
                type: "POST",
                url: "/Master/AddBulkDepartment",
                data: '{objDepartment: ' + JSON.stringify(objDepartment) + '}',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    $(".dataTables_wrapper").remove();
                    swal(response);
                    Getdata();
                    Resettable();
                    var script = document.createElement("script");
                    script.type = "text/javascript";
                    script.src = "/assets/js/jquery.dataTables.min.js";
                    document.getElementsByTagName("body")[0].appendChild(script);
                    $('#ExcelDataLoader').hide();
                },
                failure: function (response) {
                    alert(response.responseText);
                },
                error: function (response) {
                    alert(response.responseText);
                }
            });

        });
        $("#btnresetdepartment").click(function () {
            Resettable();
        });
        $('#fileUpload').change(function (e) {

            var file = e.target.files[0].name;
            $(this).next('span').text(file);
        });
    });
</script>

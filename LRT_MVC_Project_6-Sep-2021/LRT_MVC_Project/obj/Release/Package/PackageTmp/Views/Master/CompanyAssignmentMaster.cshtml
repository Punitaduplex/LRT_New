<script type="text/javascript">
    $(function () {
        $("#btnsaveuploaddb").click(function () {
            $('#ExcelDataLoader').show();
            var objcompanyassignment = {};
            objcompanyassignment.Company_Group_Name = "";
            objcompanyassignment.Company_Name = "";
            objcompanyassignment.Company_Assignment_Id = 0;
            var oTable = $('#EmpInfocompanyassigment').dataTable();
            var rowcollection = oTable.$("tr", { "page": "all" });

            rowcollection.each(function (index, elem) {
                var row = $(elem).closest("tr");
                if (objcompanyassignment.Company_Name == "") {
                    objcompanyassignment.Company_Name = row.find('td:eq(1)').text().charAt(0).toUpperCase() + row.find('td:eq(1)').text().slice(1);
                    objcompanyassignment.Company_Group_Name = row.find('td:eq(2)').text().charAt(0).toUpperCase() + row.find('td:eq(2)').text().slice(1);
                    //alert();
                }
                else {
                    objcompanyassignment.Company_Name = objcompanyassignment.Company_Name + "," + row.find('td:eq(1)').text().charAt(0).toUpperCase() + row.find('td:eq(1)').text().slice(1);
                    objcompanyassignment.Company_Group_Name = objcompanyassignment.Company_Group_Name + "," + row.find('td:eq(2)').text().charAt(0).toUpperCase() + row.find('td:eq(2)').text().slice(1);;

                }
            });

            var rowCount = $('#EmpInfocompanyassigment tr').length;
            if (rowCount <= 1) {
                swal("please upload excel file for data upload.");
                return;
            }
            if (rowCount > 1 && objcompanyassignment.Company_Name == "") {
                swal("no data in table.");
                return;
            }
            $.ajax({
                type: "POST",
                url: "/Master/AddBulkCompanyAssignment",
                data: '{objcompanyassignment: ' + JSON.stringify(objcompanyassignment) + '}',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    $(".dataTables_wrapper").remove();
                    swal(response);
                    Getdata();
                    CompanyGroup();
                    CompanyName();
                    Resettable();
                    var script = document.createElement("script");
                    script.type = "text/javascript";
                    script.src = "/assets/js/jquery.dataTables.min.js";
                    document.getElementsByTagName("body")[0].appendChild(script);
                    $('#ExcelDataLoader').hide();
                },
                failure: function (response) {
                    alert(response.responseText);
                },
                error: function (response) {
                    alert(response.responseText);
                }
            });
        });
        $("#btnresetcompanyassignment").click(function () {
            Resettable();
        });
        $('#fileUpload').change(function (e) {

            var file = e.target.files[0].name;
            $(this).next('span').text(file);
        });
    });
</script>

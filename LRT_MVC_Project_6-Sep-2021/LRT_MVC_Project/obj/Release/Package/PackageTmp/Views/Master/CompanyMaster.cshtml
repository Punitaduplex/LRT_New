<script>
    function Getdata() {
        $("#divtbl").append('<table id="EmpInfo" class="table table-bordered  table-hover"><thead><tr><th class="left_c w61_i">S. No</th><th class="left_c">Company Code</th><th class="left_c">Company Name</th><th class="left_c">Currency</th><th class="left_c">Language</th> <th>Image</th><th style="display:none;"></th> <th style="display:none;"></th><th style="display:none;"> <th style="display:none;"></th><th class="w155_i">Action</th></tr></thead><tbody></tbody></table>')
        $("#EmpInfo > tbody").html("");

        $.getJSON("/Master/CompanyMasterDetails",
            function (json) {
                var tr;

                //Append each row to html table
                for (var i = 0; i < json.length; i++) {
                    tr = $('<tr/>');
                    tr.append("<td class='left_c w61_i'>" + (i + 1) + "</td>");
                    tr.append("<td class='left_c'>" + json[i].Company_Code + "</td>");
                    tr.append("<td class='left_c'>" + json[i].Company_Name + "</td>");
                    tr.append("<td class='left_c'>" + json[i].Currency_Name + "</td>");
                    tr.append("<td class='left_c'>" + json[i].Language_Name + "</td>");
                    tr.append("<td><span class='img_wid'><img src=" + json[i].Company_Image + " ></span></td>");
                    tr.append("<td style='display:none;'>" + json[i].Company_Id + "</td>");
                    tr.append("<td style='display:none;'>" + json[i].Currency_Id + "</td>");
                    tr.append("<td style='display:none;'>" + json[i].Company_Image + "</td>");
                    tr.append("<td style='display:none;'>" + json[i].Language_Id + "</td>");
                    var status = json[i].Approve_Status;
                    var status1 = "";
                    if (status == 1) {
                        status1 = "<i class='menu-icon fa fa-check bigger-120 green'  data-toggle='tooltip' title='Approved'></i>";
                    }
                    else {
                        status1 = "<i class='menu-icon fa fa-clock-o bigger-120 green' data-toggle='tooltip' title='Pending'></i>";
                    }

                    tr.append("<td class='lst_td w155_i'><ul class='list_main'><li><a href='javascript:void(0);' class='approve tooltip-error comn_inline grn' data-rel='tooltip' >" + status1 + "</a></li><li><a href='javascript:void(0);'  id='myAnc' class='remove tooltip-error comn_inline blu' data-rel='tooltip' title='Edit'><i class='ace-icon fa fa-pencil bigger-120 edit_c'></i></a></li><li><a  href='javascript:void(0);' class='delete tooltip-error comn_inline red' data-rel='tooltip' title='Delete'><i class='menu-icon fa fa-trash bigger-120 red'></i></a></li></ul></td>");

                    $('#EmpInfo').append(tr);

                }
                $('#EmpInfo').DataTable();
            });

    }
</script>

<script>

    function Uploaddatesheet() {
        if (window.FormData !== undefined) {
            var fileUpload = $("#uploaddatesheet").get(0);
            var files = fileUpload.files;
            var fileData = new FormData();
            for (var i = 0; i < files.length; i++) {
                fileData.append(files[i].name, files[i]);

            }
            fileData.append('Datesheet', "DateSheet" + $("#ddlItemType").val().split(" ").join("") + $("#txtMPNumber").val().split(" ").join(""));

            $.ajax({
                url: '/Master/UploadFileMethod',
                type: "POST",
                contentType: false, // Not to set any content header
                processData: false, // Not to process data
                data: fileData,
                success: function (result) {
                    var f = result.split('+');
                    $("#hddatesheet").val(f[1]);
                    var path = $("#hddatesheet").val();
                    //---------------------------------------------------------------------------------------
                    swal({
                        // title: 'Auto close alert!',
                        text: f[0],
                        timer: 5000,
                        onOpen: function () {
                            swal.showLoading()
                        }
                    }).then(
                                                function () { UploadCAD() },

                                                      function (dismiss) {
                                                          if (dismiss === 'timer') {
                                                              console.log('I was closed by the timer')
                                                          }
                                                      }
                                                     )

                    //--------------------------------------------------------------------------------------
                },
                error: function (err) {
                    alert(err.statusText);
                }
            });
        } else {
            alert("FormData is not supported.");
        }
    }

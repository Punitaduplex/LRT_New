        var rowcollection = oTable.$("tr", { "page": "all" });

        var count = sessionStorage.getItem("item-count7");

        sessionStorage.setItem("item-count7", (count - 1));

        var count1 = count - 1;

        var table = $('#EmpInfocity').DataTable({
            destroy: true,
            rowReorder: true
        });
        table.on('row-reorder', function (e, diff, edit) {
            var res = '';
            for (var i = 0, ien = diff.length ; i < ien ; i++) {
                res += diff[i].newData + '=';
            }
            res = res.substring(0, res.length - 1);
            getVal(res);
        });
        $('#EmpInfocity').dataTable({
            destroy: true,
            "infoCallback": function (settings, start, end, max, total, pre) {
                return "Showing 1 to " + count1 + " entries";
            }
        });

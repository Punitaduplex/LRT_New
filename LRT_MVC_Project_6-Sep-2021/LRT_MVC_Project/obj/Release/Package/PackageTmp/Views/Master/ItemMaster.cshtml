            buttons: ["Cancel", "Yes!"],
        }).then(function (value) {
            if (value) {
                var objItem = {};
                objItem.Request_Id = $id;
                $.ajax({
                    type: "POST",
                    url: "/Master/RejectUpdatedItem",
                    data: '{objItem: ' + JSON.stringify(objItem) + '}',
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",

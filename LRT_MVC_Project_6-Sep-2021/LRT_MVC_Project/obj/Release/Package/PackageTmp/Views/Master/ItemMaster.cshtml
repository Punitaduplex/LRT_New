<script>
    function GetItemDetalAccItemIdForRefrance(itemId) {

       
        var objItem = {};
        objItem.Item_Id = itemId;
        $.ajax({
            type: "POST",
            url: "/Master/ItemDetailsForRefranceAccItemId",
            data: '{objItem: ' + JSON.stringify(objItem) + '}',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {
                $("#hditemcode").val(response.Internal_Item_Code);
                $("#txtrefrance").val(response.Internal_Item_Code);
                $("#hditemMPN").val(response.Item_Manufacturing_Part_Number);
                $("#txtDescription").val(response.Item_Description);
                $("#txtManufacture").val(response.Item_Manufacture);
                $("#txtBrand").val(response.Item_Brand);
                $("#ddlItemType").val(response.Item_Type_Id);
                //GetSubitemTypeAccItemTypeId();
                
                $("#ddlItemSub").val(response.Item_Sub_Type_Id).delay(5000);

                $("#txtMPNumber").val(response.Item_Manufacturing_Part_Number);
                var j = 0;
                var k = 0;

                var values = response.Sub_Item_Type_Field_values.split(',');
                var UOMName = response.Sub_Item_Type_Field_UOM.split(',');
               
                var optiontype = response.Sub_Item_Type_Field_Options.split(',')
              
                for (var i = 0; i < values.length; i++) {
                    // var t = $("#lbloptional" + i).text();

                    if (i % 2 == 1) {
                        var optiont = optiontype[j];
                        if (optiont == "Optional") {

                            $("#ddloptionvalue" + j).find('option[value="' + values[i] + '"]').attr("selected", true);
                           

                        }
                        else {
                           
                            $("#optionvalue" + j).val(values[i]);

                            //j = j + 1;
                        }
                        for (var l = 0; l < UOMName.length; l++) {
                          
                            if (values[i - 1].split(" ").join("") == UOMName[l].split(" ").join("")) {
                                k = k + 2;
                              
                               
                            }
                           
                        }
                      
                        $("#" + values[i - 1].split(" ").join("") + j).find('option[value="' + UOMName[k - 1] + '"]').attr("selected", "selected");;
                        j = j + 1;
                    }


                }
               
            },
            failure: function (response) {
                alert(response.responseText);
            },
            error: function (response) {
                alert(response.responseText);
            }
        });
    }

<script>
    var PClevel = '';
    document.onpaste = function (event) {
        // use event.originalEvent.clipboard for newer chrome versions
        var items = (event.clipboardData || event.originalEvent.clipboardData).items;
        console.log(JSON.stringify(items)); // will give you the mime types
        // find pasted image among pasted items
        $('#a_right').show();
        $('#a_left').show();
        var li = document.createElement('div');
        li.className = "showSlide fade";
        var img = document.createElement('img');
        var x = Math.floor((Math.random() * 100000000) + 1);
        var x1 = Math.floor((Math.random() * 100000000) + 1);
        li.id = x;
        img.id = x1;
        img.className = "img";

        var blob = null;
        for (var i = 0; i < items.length; i++) {
            if (items[i].type.indexOf("image") === 0) {
                blob = items[i].getAsFile();
                var filename = blob.name;
                var ext = filename.split('.').reverse()[0].toLowerCase();

                var fileData = new FormData();
                fileData.append('file', blob);
                fileData.append('Id', "Permit" + $("#ddlItemType").val().split(" ").join("") + $("#ddlItemSub").val().split(" ").join(""));

                $.ajax({
                    url: '/Master/UploadItemImage',
                    type: "POST",
                    contentType: false, // Not to set any content header
                    processData: false, // Not to process data
                    data: fileData,
                    success: function (result) {
                        PClevel = $("#hdimagepath").val();
                        var f = result.split('+');
                        if (PClevel == "") {
                            PClevel = f[1];
                        }
                        else {
                            PClevel = PClevel + "," + f[1];
                        }
                        
                        $("#hdimagepath").val(PClevel);


                    },
                    error: function (err) {
                        alert(err.statusText);
                    }
                });
            }
        }
        // load image if there is a pasted image
        if (blob !== null) {
            document.getElementById('div1').appendChild(li);
            document.getElementById(x).appendChild(img);
            var reader = new FileReader();
            reader.onload = function (event) {
               

                document.getElementById(x1).src = event.target.result; // data url!
            };
            var slides = document.getElementsByClassName("showSlide");
           
            //currentSlide(1);
            var slide_index = slides.length;
            if (slides.length == 1) {
                displaySlides(slide_index);
            }
            else {
                for (var i = 0; i < slides.length - 1; i++) {
                    slides[i].style.display = "none";
                }
                slides[slide_index - 1].style.display = "block";
            }

            // $('#btnUploadimage').show();

            reader.readAsDataURL(blob);
        }
    }


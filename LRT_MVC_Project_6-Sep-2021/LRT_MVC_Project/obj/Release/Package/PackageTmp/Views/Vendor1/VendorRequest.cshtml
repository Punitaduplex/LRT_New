    @*-------------------------------------------------fill countries---------------------------------------------------*@
    <script type="text/javascript">
        function CountryName() {
            var ddlcountry = $("#ddlcountry");
            var ddlVendorCountry = $("#ddlVendorCountry");
            var ddlBankCountry = $("#ddlBankCountry");
            var ddlnewcountry = $("#ddlnewcountry");
            var ddlnewsyatecountry = $("#ddlnewsyatecountry");
            ddlcountry.empty().append('<option selected="selected" value="0" disabled="disabled">Loading.....</option>');
            ddlVendorCountry.empty().append('<option selected="selected" value="0" disabled="disabled">Loading.....</option>');
            ddlBankCountry.empty().append('<option selected="selected" value="0" disabled="disabled">Loading.....</option>');
            ddlnewcountry.empty().append('<option selected="selected" value="0" disabled="disabled">Loading.....</option>');
            ddlnewsyatecountry.empty().append('<option selected="selected" value="0" disabled="disabled">Loading.....</option>');
            $.ajax({
                type: "POST",
                url: "/Master/CountryName",
                data: '{}',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    ddlcountry.empty().append('<option selected="selected" value="0">Select country name</option>');
                    ddlVendorCountry.empty().append('<option selected="selected" value="0">Select country name</option>');
                    ddlBankCountry.empty().append('<option selected="selected" value="0">Select country name</option>');
                    ddlnewcountry.empty().append('<option selected="selected" value="0">Select country name</option>');
                    ddlnewsyatecountry.empty().append('<option selected="selected" value="0">Select country name</option>');
                    $.each(response, function () {
                        ddlcountry.append($("<option></option>").val(this['Value']).html(this['Text']));
                        ddlVendorCountry.append($("<option></option>").val(this['Value']).html(this['Text']));
                        ddlBankCountry.append($("<option></option>").val(this['Value']).html(this['Text']));
                        ddlnewcountry.append($("<option></option>").val(this['Value']).html(this['Text']));
                        ddlnewsyatecountry.append($("<option></option>").val(this['Value']).html(this['Text']));
                    });
                },
                failure: function (response) {
                    alert(response.responseText);
                },
                error: function (response) {
                    alert(response.responseText);
                }
            });
        };
    </script>

            var mcapability ="";
           

            $('#EmpInfo tr').each(function () {
                if (machine == ""  && mfunction == ""  && mcapability == "") {
                    var m = $(this).find('td:eq(1) input').val();
                    var f = $(this).find('td:eq(2) input').val();
                    var c = $(this).find('td:eq(3) input').val();
                   // alert(m);
                    if (typeof m  !== "undefined") {
                        machine = $(this).find('td:eq(1) input').val();
                    }
                    if (typeof f  !== "undefined") { 
                        mfunction = $(this).find('td:eq(2) input').val();
                    }
                    if (typeof c  !== "undefined")
                    {
                        mcapability = $(this).find('td:eq(3) input').val();
                    }
                }
                else {
                    var m = $(this).find('td:eq(1) input').val();
                    var f = $(this).find('td:eq(2) input').val();
                    var c = $(this).find('td:eq(3) input').val();
                    if (m != "" || f != "" || c != "") {
                        machine = machine + "," + $(this).find('td:eq(1) input').val();
                        mfunction = mfunction + "," + $(this).find('td:eq(2) input').val();
                        mcapability = mcapability + "," + $(this).find('td:eq(3) input').val();
                       
                        

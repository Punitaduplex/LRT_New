                        drawingURL = $(tr).find('td:eq(5)').text();
                        CadFileType = $(tr).find('td:eq(6)').text();
                    }
                    if (format == "PDF") {
                        drawingno = $(tr).find('td:eq(3)').text();
                        drawingURL = $(tr).find('td:eq(5)').text();
                        CadFileType = $(tr).find('td:eq(6)').text();
                    }
                    if (format == "CAD") {

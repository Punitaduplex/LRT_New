                        drawingno = drawingno + "," + $(tr).find('td:eq(1)').text();
                        drawingURL = drawingURL + "," + $(tr).find('td:eq(5)').text();
                        CadFileType = CadFileType + "," + $(tr).find('td:eq(6)').text();

            }

            if (excelRows[i].BranchName == undefined) {
                Branch_Name = "";
            }
            else {
                Branch_Name = excelRows[i].BranchName;
            }
            if (excelRows[i].Currency == undefined) {

                C_Currency = "";
            }
            else {
                C_Currency = excelRows[i].Currency;
            }

            if (excelRows[i].Language == undefined) {

                L_Language = "";
            }
            else {
                L_Language = excelRows[i].Language;
            }
            if (excelRows[i].GSTNumber == undefined) {
                GST_Number = "";
            }
            else {
                GST_Number = excelRows[i].GSTNumber;
            }
            if (excelRows[i].SSTNumber == undefined) {
                SST_Number = "";
            }
            else {
                SST_Number = excelRows[i].SSTNumber;
            }
            if (excelRows[i].IsPlant == undefined) {
                Is_Plant = "";
            }
            else {
                Is_Plant = excelRows[i].IsPlant;
            }
            if (excelRows[i].IsWarehouse == undefined) {
                Is_Warehouse = "";
            }
            else {
                Is_Warehouse = excelRows[i].IsWarehouse;
            }

            tr = $('<tr/>');
            tr.append("<td class='left_c w61_i'>" + (i + 1) + "</td>");
            tr.append("<td class='left_c'>" + Branch_Code + "</td>");

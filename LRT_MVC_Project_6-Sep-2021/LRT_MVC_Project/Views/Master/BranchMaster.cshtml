        $("#EmpInfobranch > tbody").html("");
        var count = excelRows.length;
        for (var i = 0; i < excelRows.length; i++) {
            var Branch_Code = "";
            var Branch_Name = "";
            var C_Currency = "";
            var L_Language = "";
            var GST_Number = "";
            var SST_Number = "";
            var Is_Plant = "";
            var Is_Warehouse = ""
            // alert(excelRows[i].SSTNumber)
            if (excelRows[i].BranchCode == undefined) {
                Branch_Code = "";
            }
            else {

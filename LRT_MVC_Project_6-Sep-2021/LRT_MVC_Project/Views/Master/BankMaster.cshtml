<script type="text/javascript">
    $(function () {
        $("#btnsaveuploaddb").click(function () {
            $('#ExcelDataLoader').show();
            var objbank = {};
            objbank.Bank_Code = "";
            objbank.Bank_Name = "";
            objbank.Swift_Code ="";// $("#txtamount").val();
            objbank.Bank_Id = 0;
            var oTable = $('#EmpInfobank').dataTable();
            var rowcollection = oTable.$("tr", { "page": "all" });

            rowcollection.each(function (index, elem) {
                var row = $(elem).closest("tr");

                if (objbank.Bank_Name == "") {
                    objbank.Bank_Code = row.find('td:eq(1)').text().toUpperCase();
                    objbank.Bank_Name = row.find('td:eq(2)').text().charAt(0).toUpperCase() + row.find('td:eq(2)').text().slice(1);
                    objbank.Swift_Code = row.find('td:eq(3)').text().toUpperCase() ;
                }
                else {
                    objbank.Bank_Code = objbank.Bank_Code + "," + row.find('td:eq(1)').text().toUpperCase();
                    objbank.Bank_Name = objbank.Bank_Name + "," + row.find('td:eq(2)').text().charAt(0).toUpperCase() + row.find('td:eq(2)').text().slice(1);;
                    objbank.Swift_Code = objbank.Swift_Code + "," + row.find('td:eq(3)').text().toUpperCase();
                }
            });
            var rowCount = $('#EmpInfobank tr').length;
            if (rowCount <= 1) {
                swal("please upload excel file for data upload.");
                return;
            }
            if (rowCount > 1 && objbank.Bank_Name == "") {
                swal("no data in table.");
                return;
            }

            $.ajax({
                type: "POST",
                url: "/Master/AddBulkBank",
                data: '{objbank: ' + JSON.stringify(objbank) + '}',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    $(".dataTables_wrapper").remove();
                    swal(response);
                    Getdata();
                    Resettable();
                    var script = document.createElement("script");
                    script.type = "text/javascript";
                    script.src = "/assets/js/jquery.dataTables.min.js";
                    document.getElementsByTagName("body")[0].appendChild(script);
                    $('#ExcelDataLoader').hide();
                },
                failure: function (response) {
                    alert(response.responseText);
                },
                error: function (response) {
                    alert(response.responseText);
                }
            });
        });
        $("#btnresetbank").click(function () {
            Resettable();
        });
        $('#fileUpload').change(function (e) {

            var file = e.target.files[0].name;
            $(this).next('span').text(file);
        });
    });
</script>

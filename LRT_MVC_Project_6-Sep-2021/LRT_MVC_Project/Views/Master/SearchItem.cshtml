<script type="text/javascript">
    function copyToClipboard(element) {
        //alert($(element).text());
        //alert($(element).text());
        var $temp = $("<input>");
        $("body").append($temp);
        $temp.val($(element).text()).select();
        document.execCommand("copy");
        $temp.remove();
        Getdata($(element).text());
        swal("copied part number " + $(element).text());
    }
</script>


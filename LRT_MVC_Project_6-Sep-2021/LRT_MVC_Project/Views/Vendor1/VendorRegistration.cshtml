@*---------------------------------------------Add Capability detail-----------------------------------------*@
<script>
    $(document).ready(function () {
        $("#btnCapability").click(function () {
            $("#EmpInfo > tbody").html("");
            var tr;
            $('#dialogdiv').modal('show');

            var machinename = localStorage.getItem("machine_name");
            var funcationname = localStorage.getItem("function_name");
            var capabilityname = localStorage.getItem("capability_name");
            if (machinename != null) {
                var mval = machinename.split(',');

            }
            if (funcationname != null) {
                var fval = funcationname.split(',');

            }
            if (capabilityname != null) {
                var cval = capabilityname.split(',');

            }

            for (var i = 0; i < 10; i++) {
                tr = $('<tr/>');

                tr.append("<td style='text-align:center'>" + (i + 1) + "</td>");
                if (machinename != null) {
                    if ((typeof mval[i] !== "undefined")) {
                        tr.append("<td><input value='" + mval[i] + "' type='text' id='txtname" + i + "' class='col-xs-12 col-sm-12' /></td>");
                    } else {
                        tr.append("<td><input type='text' id='txtname" + i + "' class='col-xs-12 col-sm-12' /></td>");
                    }
                } else {
                    tr.append("<td><input type='text' id='txtname" + i + "' class='col-xs-12 col-sm-12' /></td>");
                }
                if (funcationname != null) {
                    if ((typeof fval[i] !== "undefined")) {
                        tr.append("<td><input value='" + fval[i] + "' type='text' id='txtfunction" + i + "' class='col-xs-12 col-sm-12'/></td>");
                    }
                    else {
                        tr.append("<td><input type='text' id='txtfunction" + i + "' class='col-xs-12 col-sm-12'/></td>");
                    }
                }
                else {
                    tr.append("<td><input type='text' id='txtfunction" + i + "' class='col-xs-12 col-sm-12'/></td>");
                }
                if (capabilityname != null) {
                    if ((typeof cval[i] !== "undefined")) {
                        tr.append("<td><input  value='" + cval[i] + "' type='text' id='txtcapability" + i + "' class='col-xs-12 col-sm-12'/></td>");
                    }
                    else {
                        tr.append("<td><input type='text' id='txtcapability" + i + "' class='col-xs-12 col-sm-12'/></td>");
                    }
                }
                else {
                    tr.append("<td><input type='text' id='txtcapability" + i + "' class='col-xs-12 col-sm-12'/></td>");
                }


                $('table').append(tr);
            }
            $('#EmpInfo').DataTable();
            localStorage.removeItem("machine_name");
            localStorage.removeItem("function_name");
            localStorage.removeItem("capability_name");

        });

    })
</script>


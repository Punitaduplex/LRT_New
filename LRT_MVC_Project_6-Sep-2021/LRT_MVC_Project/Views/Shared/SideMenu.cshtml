<script type="text/JavaScript">
    $(document).ready(function () {

        $("#myTab").find("li>ul").addClass("active");
        $("#myTab").find("li:first ul").show();
        var checkCookie1 = sessionStorage.getItem("nav-item1");
        var checkCookie = sessionStorage.getItem("nav-item");
        if (checkCookie != "") {
            $('#myTab>li>ul>li:eq(' + checkCookie + ')').find("ul").toggleClass("close");
            var ul1 = $("#myTab").find("li").length;
            for (i = 0; i < ul1; i++) {
                if (i != checkCookie) {
                    $('#myTab>li>ul>li:eq(' + i + ')').find(" span").nextAll().toggleClass("dnone");
                }
            }
        }
        $('#myTab>li>ul>li>ul>li:eq(' + checkCookie1 + ')').find("a").addClass("active2");
    });

</script>


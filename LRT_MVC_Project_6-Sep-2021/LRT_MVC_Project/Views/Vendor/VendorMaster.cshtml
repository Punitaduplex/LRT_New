            var fileUpload = $("#product_catalog").get(0);
            var files = fileUpload.files;
            var fileData = new FormData();
            for (var i = 0; i < files.length; i++) {
                fileData.append(files[i].name, files[i]);

            }
            fileData.append('catelog', "catelog" + $("#txtcompanyregistrationno").val().split(" ").join("") + $("#txtVendorcompanyname").val().split(" ").join(""));

            $.ajax({
                url: '/Vendor/UploadProductCatalogMethod',
                type: "POST",
                contentType: false, // Not to set any content header
                processData: false, // Not to process data
                data: fileData,
                success: function (result) {
                    var f = result.split('+');
                    $("#hdproductcatalogFile").val(f[1]);
                    var path = $("#hdproductcatalogFile").val();
                    //alert(path);
                    swal(f[0]);

                    // $("#spin").hide();
                },
                error: function (err) {
                    alert(err.statusText);
                }
            });
        } else {
            alert("FormData is not supported.");
        }
    }
</script>

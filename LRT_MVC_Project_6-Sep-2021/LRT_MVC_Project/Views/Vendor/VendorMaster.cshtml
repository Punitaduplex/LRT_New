        $("#btnsaveVendorinvitation").click(function (e) {
            e.preventDefault();
            $('#ExcelDataLoader').show();
            var objVendor = {};
            objVendor.Vendor_Company_Name = $("#txtcompanyname").val().charAt(0).toUpperCase() + $("#txtcompanyname").val().slice(1);
            objVendor.Vendor_Contact_Person = $("#txtcontactperson").val().charAt(0).toUpperCase() + $("#txtcontactperson").val().slice(1);
            objVendor.Vendor_Email_ID = $("#txtemailid").val();
            objVendor.Vendor_Invitation_Id = $("#hdvendorinvitation").val();

            var list = [];

            $("input:checkbox[name=form-field-checkbox]:checked").each(function () {
                list.push({ Vendor_Type_Id: $(this).val() })
            });


            objVendor.Type_List = list;
            if (objVendor.Vendor_Company_Name == "" || objVendor.Vendor_Company_Name == null) {
                swal("Please enter company name.");
                $("#txtcompanyname").focus();
                $("#ExcelDataLoader").hide();
                return;
            }
            if (objVendor.Vendor_Contact_Person == "" || objVendor.Vendor_Contact_Person == null) {
                swal("Please enter contact person name.");
                $("#txtcontactperson").focus();
                $("#ExcelDataLoader").hide();
                return;
            }
            if (objVendor.Vendor_Email_ID == "" || objVendor.Vendor_Email_ID == null) {
                swal("Please enter vendor email id.");
                $("#txtemailid").focus();
                $("#ExcelDataLoader").hide();
                return;
            }
            var emailReg = /^([\w-\.]+@@([\w-]+\.)+[\w-]{2,4})?$/;
            var emailaddressVal = objVendor.Vendor_Email_ID;
            if (!emailReg.test(emailaddressVal)) {
                $("#txtemailid").after(swal("Please enter valid email address."));
                $("#ExcelDataLoader").hide();
                return;
            }
            $.ajax({
                type: "POST",
                url: "/Vendor/AddVendorInvitation",
                data: '{objVendor: ' + JSON.stringify(objVendor) + '}',
                contentType: "application/json; charset=utf-8",
                dataType: "json",

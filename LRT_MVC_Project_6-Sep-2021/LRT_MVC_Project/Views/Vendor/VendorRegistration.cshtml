<script>
    function GetVendorInvitationdata() {
        $("#divtblinvitation").append('<table id="EmpInfoInvitation" class="able table-bordered table-hover"><thead><tr><th>S. No</th><th>Company Name</th><th>Contact Person</th><th>Email ID</th><th>Action</th><th class="hide_me"></th></tr></thead> <tbody></tbody> </table>')
        $("#EmpInfoInvitation > tbody").html("");
        $.getJSON("/Vendor/VendorInvitationDetails",
            function (json) {
               
                var tr;
                img = "<img src='/Images/plus.png'/>";
                //Append each row to html table
                for (var i = 0; i < json.length; i++) {
                    tr = $('<tr/>');
                    tr.append("<td><label class='pos-rel'>" + (i + 1) + img + "</lable></td>");
                    tr.append("<td><label class='pos-rel'>" + json[i].Vendor_Company_Name + "</lable></td>");
                    tr.append("<td><label class='pos-rel'>" + json[i].Vendor_Contact_Person + "</lable></td>");
                    tr.append("<td ><label class='pos-rel'>" + json[i].Vendor_Email_ID + "</lable></td>");
                    //tr.append("<td style='display:none'>" + json[i].Access_Permission + "</td>");

                    var status = json[i].Approve_Status;
                  
                    var status1 = "";
                    if (status == 1) {
                        status1 = "<i class='menu-icon fa fa-check bigger-120 green' data-rel='tooltip' title='Approved'></i>";
                    }
                    else {
                        status1 = "<i class='menu-icon fa fa-clock-o bigger-120 green' data-rel='tooltip' title='Pending'></i>";
                    }
                    tr.append("<td><ul class='list_main'><li><a href='javascript:void(0);' class='approveinvite tooltip-error comn_inline grn' data-rel='tooltip'>" + status1 + "</a></li><li ><a href='javascript:void(0);'  id='myAnc' class='editinvite tooltip-error comn_inline blu' data-rel='tooltip'><i class='ace-icon fa fa-pencil bigger-120 edit_c' data-rel='tooltip' title='Edit'></i></a></li></ul></td>");
                    //tr.append("<td class='lst_td'><ul class='list_main'><li><a href='' class='tooltip-error comn_inline grn' data-rel='tooltip' title='Approve by it'><i class='menu-icon fa fa-check grn'></i>Approve</a></li><li><a href='#' class='tooltip-error comn_inline blu' data-rel='tooltip' title='Edit by it'><i class='ace-icon fa fa-pencil bigger-120 edit_c'></i>Edit</a></li><li><a href='' class='tooltip-error comn_inline red' data-rel='tooltip' title='Delete by it'><i class='menu-icon fa fa-trash bigger-120 red'></i>Delete</a></li></ul></td>");<li><a  href='javascript:void(0);' class='detail tooltip-error comn_inline red' data-rel='tooltip'><i class='menu-icon fa fa-trash bigger-120 red'></i>Detail</a></li>
                    tr.append("<td class='hide_me'>" + json[i].Vendor_Invitation_Id + "</td>");
                    $('#EmpInfoInvitation').append(tr);

                }
                $('#EmpInfoInvitation').DataTable({

                    // responsive: true,
                    colReorder: true,
                    destroy: true
                  
                });
            });

    };
</script>

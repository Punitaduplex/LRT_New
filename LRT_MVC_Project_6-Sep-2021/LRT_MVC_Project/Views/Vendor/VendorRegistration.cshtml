<script>
    function Getnesteddata(invitationId) {
        $("#divnestedtbl" + invitationId).append('<table id="NestedEmpInfo' + invitationId + '" class="table-hovertable table-bordered padding_15"><thead><tr><th>Vendor Type Name </th> </tr></thead> <tbody></tbody> </table>')
        $("#NestedEmpInfo" + invitationId + " > tbody").html("");
        var objVendor = {};

        objVendor.Vendor_Invitation_Id = invitationId;

        $.ajax({
            type: "POST",
            url: "/Vendor/VendorTypeAccInvitationId",
            data: '{objVendor: ' + JSON.stringify(objVendor) + '}',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {
                // alert(response.length);
                if (response.length > 0) {
                    for (var i = 0; i < response.length; i++) {
                        tr = $('<tr/>');
                        tr.append("<td><label class='pos-rel'>" + response[i].Vendor_Type + "</label></td>");
                        $('#NestedEmpInfo' + invitationId).append(tr);
                    }
                    $('#NestedEmpInfo' + invitationId).DataTable();
                }
                else {
                    $("#divnestedtbl" + invitationId).empty();
                }
            },
            failure: function (response) {
                alert(response.responseText);
            },
            error: function (response) {
                alert(response.responseText);
            }
        });
    };
</script>


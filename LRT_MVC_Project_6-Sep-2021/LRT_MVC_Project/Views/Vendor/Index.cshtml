<script type="text/javascript">

    function Circularchar() {
        let myChart = document.getElementById('myChart_bar').getContext('2d');
        var sub_array = [];
        var super_array = [];
        var sub_array1 = [];
        var super_array1 = [];
        $.getJSON("/Vendor1/VendorStatuschart",
            function (json) {

                for (var i = 0; i < json.length; i++) {
                    sub_array.push("" + json[i].Label1 + "");
                    super_array.push(sub_array);
                    sub_array1.push("" + json[i].Count + "");
                    super_array1.push(sub_array1);
                }




                new Chart(myChart_bar, {
                    type: 'pie',
                    data: {
                        labels:

                            sub_array

                         ,


                        datasets: [{
                            label: 'Population',
                            data:


                                sub_array1,

                            // backgroundColor:'green',
                            backgroundColor: [
                                '#20336e',
                                '#2589ad',
                                '#86ddd4',

                            ],

                            borderWidth: 1,
                            borderColor: '#777',
                            hoverBorderWidth: 3,
                            hoverBorderColor: '#000',
                        }],


                    },

                    options: {
                        plugins: {
                            title: {
                                display: true,
                                text: 'Mode of Registration',
                                fontSize: 35,
                                color: '#000',
                            }
                        },
                        animations: {
                            tension: {
                                duration: 1000,
                                easing: 'linear',
                                from: 1,
                                to: 0,
                                loop: true
                            }
                        },

                    },
                });


            });

    }
</script>
 

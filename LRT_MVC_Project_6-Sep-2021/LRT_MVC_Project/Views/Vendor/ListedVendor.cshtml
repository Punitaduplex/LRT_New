<script>
    function Getdata() {
        $("#divtbl").append('<table id="listedvendor" class="table table-bordered  table-hover"><thead><tr><th><input type="checkbox" id="chkAll"</th><th class="left_c">Name</th><th class="left_c">AVL/SBL Number</th><th class="left_c">Company Name</th><th class="left_c">Email</th> <th class="left_c">Work Phone</th><th class="w155_i">Action</th><th style="display:none"></tr></thead> <tbody></tbody> </table>')

        $("#listedvendor > tbody").html("");
        var objVendor = {};

        var objlistedvendor = {};
        objlistedvendor.Vendor_Name = $("#ddlvendorName").val();
        objlistedvendor.Vendor_Company_Name = $("#ddlcompanyname").val();

        objlistedvendor.Vendor_Contact_Person = $("#ddlWorkPhone").val();
        objlistedvendor.Vendor_Email_ID = $("#ddlEmail").val();
        // alert();
        objlistedvendor.Vendor_Id = $("#hdlistedvendorid").val();

        $.ajax({
            type: "POST",
            url: "/Vendor/SearchListedVendorbysearch",
            data: '{objlistedvendor: ' + JSON.stringify(objlistedvendor) + '}',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {
               
                if (response.length > 0) {
                    for (var i = 0; i < response.length; i++) {
                        tr = $('<tr/>');
                        tr.append("<td><input type='checkbox' name='listedVendor' class='ace'/></td>");
                        tr.append("<td><label class='pos-rel'>" + response[i].Vendor_Name + "</label></td>");
                        tr.append("<td class='left_c'></td>");
                        tr.append("<td><label class='pos-rel'>" + response[i].Vendor_Company_Name + "</label></td>");
                        tr.append("<td><label class='pos-rel'>" + response[i].Vendor_Email_ID + "</label></td>");
                        tr.append("<td><label class='pos-rel'>" + response[i].Vendor_Contact_Person + "</label></td>");
                        tr.append("<td class='lst_td w155_i'><ul class='list_main'><li ><a href='javascript:void(0);'  id='myAnc' class='remove tooltip-error comn_inline blu' data-rel='tooltip'><i class='ace-icon fa fa-pencil bigger-120 edit_c' data-rel='tooltip' title='Edit'></i></a><a  href='javascript:void(0);' class='delete tooltip-error comn_inline red' data-rel='tooltip' title='List'><i class='menu-icon fa fa-eye bigger-120 green'></i></a><a  href='javascript:void(0);' class='List tooltip-error comn_inline red' data-rel='tooltip' title='List'><i class='menu-icon fa fa-clock-o bigger-120 Red '></i></a></li></ul></td>");
                        tr.append("<td class='hide_me'>" + response[i].Vendor_Id + "</td>");
                        $('#listedvendor').append(tr);
                    }
                    $('#listedvendor').DataTable({


                        colReorder: true,
                        destroy: true

                    });
                }
                else {
                    $("#divtbl").empty();
                }
            },
            failure: function (response) {
                alert(response.responseText);
            },
            error: function (response) {
                alert(response.responseText);
            }
        });
    };
</script>








<script>
    $(function () {
        $('body').on('click', '#chkAll', function () {
            var ListedvendorTable = $('#listedvendor').dataTable();
            var rowselction = ListedvendorTable.$("input[name='listedVendor']", { "page": "all" });

            if ($(this).is(':checked')) {
                rowselction.each(function (index, elem) {
                    var row = $(elem).closest("tr");
                    row.find("td:eq(0)").find('input[type="checkbox"]').prop('checked', true);
                });

            }
            else {
                rowselction.each(function (index, elem) {
                    var row = $(elem).closest("tr");
                    row.find("td:eq(0)").find('input[type="checkbox"]').prop('checked', false);
                });
            }

        })
    });
     
</script>
 

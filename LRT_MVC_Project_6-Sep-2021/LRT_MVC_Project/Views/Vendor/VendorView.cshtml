<script>
    $(document).ready(function () {

        //var $row = $(this).closest("tr");    // Find the row
        //var $id = $row.find('td:eq(7)').text();

        //var objlistedvendor = {};
        //objlistedvendor.Vendor_Id = $id;
       
        var baseUrl = (window.location).href;
        var Id = baseUrl.substring(baseUrl.lastIndexOf('/') + 1);

        var objlistedvendor = {};
        objlistedvendor.Vendor_Id = Id;
        $.ajax({
            type: "POST",
            url: "/Vendor/ListedVendordetailsForPopup",
            data: '{objlistedvendor: ' + JSON.stringify(objlistedvendor) + '}',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {

                $("#lblcompanyname").empty();
                if (response.Vendor_Name == "") {
                    $("#lblVendorName").text("NA");
                } else {
                    $("#lblVendorName").text(response.Vendor_Name);
                }

                $("#lblBillinguserName").empty();
                if (response.Vendor_Name == "") {
                    $("#lblBillinguserName").text("NA");
                } else {
                    $("#lblBillinguserName").text(response.Vendor_Name);
                }


                $("#lblBillingAddress").empty();
                if (response.Billingaddress == "") {
                    $("#lblBillingAddress").text("NA");
                } else {
                    $("#lblBillingAddress").text(response.Billingaddress);
                }
                $("#lblbillingsidephoneno").empty();
                if (response.Vendor_Contact_Person == "") {
                    $("#lblbillingsidephoneno").text("NA");
                } else {
                    $("#lblbillingsidephoneno").text(response.Vendor_Contact_Person);
                }





                $("#lblShippinguserName").empty();
                if (response.Vendor_Name == "") {
                    $("#lblShippinguserName").text("NA");
                } else {
                    $("#lblShippinguserName").text(response.Vendor_Name);
                }


                $("#lblShippingAddress").empty();
                if (response.Billingaddress == "") {
                    $("#lblShippingAddress").text("NA");
                } else {
                    $("#lblShippingAddress").text(response.ShippingAddress);
                }
                $("#lblShippingsidephoneno").empty();
                if (response.Vendor_Contact_Person == "") {
                    $("#lblShippingsidephoneno").text("NA");
                } else {
                    $("#lblShippingsidephoneno").text(response.Vendor_Contact_Person);
                }

                $("#lblcurrencycode").empty();
                if (response.currency_code == "") {
                    $("#lblcurrencycode").text("NA");
                } else {
                    $("#lblcurrencycode").text(response.currency_code);
                }

                $("#lblcurrencysymbol").empty();
                if (response.currency_symbol == "") {
                    $("#lblcurrencysymbol").text("NA");
                } else {
                    $("#lblcurrencysymbol").text(response.currency_symbol);
                }
                $("#lblVendorAddress").empty();
                if (response.Vendor_Address == "") {
                    $("#lblVendorAddress").text("NA");
                } else {
                    $("#lblVendorAddresss").text(response.Vendor_Address);
                }

            },
            failure: function (response) {
                alert(response.responseText);
            },
            error: function (response) {
                alert(response.responseText);
            }
        });
    });
</script>



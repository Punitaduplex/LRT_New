<script type="text/javascript">
    $(function () {
        $("#btnupdatecompanyprofile").click(function () {

            var objvendor = {};
            objvendor.Company_name = $("#txtcomapnyname").val().toUpperCase();
            objvendor.CompanyAddress = $("#txtcompanylocation").val().charAt(0).toUpperCase() + $("#txtcompanylocation").val().slice(1);
            objvendor.Company_Telephone_number = $("#txtcompanyphoneno").val().charAt(0).toUpperCase() + $("#txtcompanyphoneno").val().slice(1);
            var baseUrl = (window.location).href;
            var Id = baseUrl.substring(baseUrl.lastIndexOf('/') + 1);
            objvendor.vendor_id = Id;
            alert(objvendor.Company_Telephone_number);
            if (objvendor.Company_name == "" || objvendor.Company_name == null) {
                swal("Please enter Company name.");

                $("#txtcomapnyname").focus();
                return;
            }
            if (objvendor.CompanyAddress == "" || objvendor.CompanyAddress == null) {
                swal("Please enter Company address.");
                $("#txtcompanylocation").focus();
                return;
            }
            if (objvendor.Company_Telephone_number == "" || objvendor.Company_Telephone_number == null) {
                swal("Please enter Company Phone no.");
                $("#txtcompanyphoneno").focus();
                return;
            }

            $.ajax({
                type: "POST",
                url: "/Vendor/Upadtevendorcompanyprofile ",
                data: '{objvendor: ' + JSON.stringify(objvendor) + '}',
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    
                    $(".dataTables_wrapper").remove();
                    swal(response);
                    Getdata();
                    Resetdata1();
                    var script = document.createElement("script");
                    script.type = "text/javascript";
                    script.src = "/assets/js/jquery.dataTables.min.js";
                    document.getElementsByTagName("body")[0].appendChild(script);

                },
                failure: function (response) {
                    alert(response.responseText);
                },
                error: function (response) {
                    alert(response.responseText);
                }
            });
        });
    });
</script>
<script type="text/javascript">
    //$(function () {
    //    //  alert();

    //});
    function GetBank() {
        var ddlBank = $("#ddlbankname");

        ddlBank.empty().append('<option selected="selected" value="0" disabled="disabled">Loading.....</option>');
        $.ajax({
            type: "POST",
            url: "/Master/BankName",
            data: '{}',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {

                ddlBank.empty().append('<option selected="selected" value="0">Select Bank</option>');
                $.each(response, function () {

                    ddlBank.append($("<option></option>").val(this['Value']).html(this['Text']));
                });
            },
            failure: function (response) {
                alert(response.responseText);
            },
            error: function (response) {
                alert(response.responseText);
            }
        });
    }
</script>


